create database aditya

create table users(
	name varchar(255),
	salary int
)

insert into users values('Aditya',25000)
insert into users values('Ashish',34000);
insert into users values('Vishal',20000);
insert into users values('Gaurav',45000);
insert into users values('akshay',28000);
insert into users values('Nikhil',10000);
insert into users values('Rahul',18000);

update users set salary=75000 where name='Vishal'

delete from users where name='akshay'

select name,salary from users;
select name,salary from users where salary<30000;
select name,salary from users where salary>30000 and salary<80000;

select name,salary from users order by salary asc;
select name,salary from users where name like 'a%';
select name,salary from users where name like 'as%';

select count(name) from users;
select sum(salary) from users;
select max(salary) from users;